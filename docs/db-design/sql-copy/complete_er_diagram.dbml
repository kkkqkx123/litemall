
Table litemall_ad {
  id integer [pk, not null]
  name varchar [not null] [note: '广告标题']
  link varchar [not null] [note: '所广告的商品页面或者活动页面链接地址']
  url varchar [not null] [note: '广告宣传图片']
  position boolean [note: '广告位置：1则是首页']
  content varchar [note: '活动内容']
  start_time datetime [note: '广告开始时间']
  end_time datetime [note: '广告结束时间']
  enabled boolean [note: '是否启动']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '广告标题'
}

Table litemall_address {
  id integer [pk, not null]
  name varchar [not null] [note: '收货人名称']
  user_id integer [ref, not null] [note: '用户表的用户ID']
  province varchar [not null] [note: '行政区域表的省ID']
  city varchar [not null] [note: '行政区域表的市ID']
  county varchar [not null] [note: '行政区域表的区县ID']
  address_detail varchar [not null] [note: '详细收货地址']
  area_code char [note: '地区编码']
  postal_code char [note: '邮政编码']
  tel varchar [not null] [note: '手机号码']
  is_default boolean [not null] [note: '是否默认地址']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '收货人名称'
}

Table litemall_admin {
  id integer [pk, not null]
  username varchar [not null] [note: '管理员名称']
  password varchar [not null] [note: '管理员密码']
  last_login_ip varchar [note: '最近一次登录IP地址']
  last_login_time datetime [note: '最近一次登录时间']
  avatar varchar [note: '头像图片']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  role_ids varchar [note: '角色列表']
  Note: '管理员名称'
}

Table litemall_aftersale {
  id integer [pk, not null]
  aftersale_sn varchar [ref] [note: '售后编号']
  order_id integer [ref, not null] [note: '订单ID']
  user_id integer [ref, not null] [note: '用户ID']
  type smallint [note: '售后类型，0是未收货退款，1是已收货（无需退货）退款，2用户退货退款']
  reason varchar [note: '退款原因']
  pictures varchar [note: '退款凭证图片链接数组']
  comment varchar [note: '退款说明']
  status smallint [note: '售后状态，0是可申请，1是用户已申请，2是管理员审核通过，3是管理员退款成功，4是管理员审核拒绝，5是用户已取消']
  handle_time datetime [note: '管理员操作时间']
  add_time datetime [note: '添加时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '售后编号'
}

Table litemall_brand {
  id integer [pk, not null]
  name varchar [not null] [note: '品牌商名称']
  desc varchar [not null] [note: '品牌商简介']
  pic_url varchar [not null] [note: '品牌商页的品牌商图片']
  sort_order boolean
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '品牌商名称'
}

Table litemall_cart {
  id integer [pk, not null]
  user_id integer [ref] [note: '用户表的用户ID']
  goods_id integer [ref] [note: '商品表的商品ID']
  goods_sn varchar [ref] [note: '商品编号']
  goods_name varchar [note: '商品名称']
  product_id integer [ref] [note: '商品货品表的货品ID']
  number smallint [note: '商品货品的数量']
  specifications varchar [note: '商品规格值列表，采用JSON数组格式']
  checked boolean [note: '购物车中商品是否选择状态']
  pic_url varchar [note: '商品图片或者商品货品图片']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '用户表的用户ID'
}

Table litemall_category {
  id integer [pk, not null]
  name varchar [not null] [note: '类目名称']
  keywords varchar [not null] [note: '类目关键字，以JSON数组格式']
  desc varchar [note: '类目广告语介绍']
  pid integer [not null] [note: '父类目ID']
  icon_url varchar [note: '类目图标']
  pic_url varchar [note: '类目图片']
  level varchar
  sort_order boolean [note: '排序']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '类目名称'
}

Table litemall_collect {
  id integer [pk, not null]
  user_id integer [ref, not null] [note: '用户表的用户ID']
  value_id integer [ref, not null] [note: '如果type=0，则是商品ID；如果type=1，则是专题ID']
  type boolean [not null] [note: '收藏类型，如果type=0，则是商品ID；如果type=1，则是专题ID']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '用户表的用户ID'
}

Table litemall_comment {
  id integer [pk, not null]
  value_id integer [ref, not null] [note: '如果type=0，则是商品评论；如果是type=1，则是专题评论。']
  type boolean [not null] [note: '评论类型，如果type=0，则是商品评论；如果是type=1，则是专题评论；']
  content varchar [note: '评论内容']
  admin_content varchar [note: '管理员回复内容']
  user_id integer [ref, not null] [note: '用户表的用户ID']
  has_picture boolean [note: '是否含有图片']
  pic_urls varchar [note: '图片地址列表，采用JSON数组格式']
  star smallint [note: '评分， 1-5']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '如果type=0，则是商品评论；如果是type=1，则是专题评论。'
}

Table litemall_coupon {
  id integer [pk, not null]
  name varchar [not null] [note: '优惠券名称']
  desc varchar [note: '优惠券介绍，通常是显示优惠券使用限制文字']
  tag varchar [note: '优惠券标签，例如新人专用']
  total integer [not null] [note: '优惠券数量，如果是0，则是无限量']
  limit smallint [note: '用户领券限制数量，如果是0，则是不限制；默认是1，限领一张.']
  type smallint [note: '优惠券赠送类型，如果是0则通用券，用户领取；如果是1，则是注册赠券；如果是2，则是优惠券码兑换；']
  status smallint [note: '优惠券状态，如果是0则是正常可用；如果是1则是过期; 如果是2则是下架。']
  goods_type smallint [note: '商品限制类型，如果0则全商品，如果是1则是类目限制，如果是2则是商品限制。']
  goods_value varchar [note: '商品限制值，goods_type如果是0则空集合，如果是1则是类目集合，如果是2则是商品集合。']
  code varchar [note: '优惠券兑换码']
  time_type smallint [note: '有效时间限制，如果是0，则基于领取时间的有效天数days；如果是1，则start_time和end_time是优惠券有效期；']
  days smallint [note: '基于领取时间的有效天数days。']
  start_time datetime [note: '使用券开始时间']
  end_time datetime [note: '使用券截至时间']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '优惠券名称'
}

Table litemall_coupon_user {
  id integer [pk, not null]
  user_id integer [ref, not null] [note: '用户ID']
  coupon_id integer [ref, not null] [note: '优惠券ID']
  used_time datetime [note: '使用时间']
  start_time datetime [note: '有效期开始时间']
  end_time datetime [note: '有效期截至时间']
  order_id integer [ref] [note: '订单ID']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '用户ID'
}

Table litemall_feedback {
  id integer [pk, not null]
  user_id integer [ref, not null] [note: '用户表的用户ID']
  username varchar [not null] [note: '用户名称']
  mobile varchar [not null] [note: '手机号']
  feed_type varchar [not null] [note: '反馈类型']
  content varchar [not null] [note: '反馈内容']
  status integer [not null] [note: '状态']
  has_picture boolean [note: '是否含有图片']
  pic_urls varchar [note: '图片地址列表，采用JSON数组格式']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '用户表的用户ID'
}

Table litemall_footprint {
  id integer [pk, not null]
  user_id integer [ref, not null] [note: '用户表的用户ID']
  goods_id integer [ref, not null] [note: '浏览商品ID']
  add_time datetime [note: '创建时间']
