Table litemall_topic {
  id integer [pk, not null]
  title varchar [not null] [note: '专题标题']
  subtitle varchar [note: '专题子标题']
  content text [note: '专题内容，富文本格式']
  read_count varchar [note: '专题阅读量']
  pic_url varchar [note: '专题图片']
  sort_order integer [note: '排序']
  goods varchar [note: '专题相关商品，采用JSON数组格式']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '专题标题'
}

Table litemall_user {
  id integer [pk, not null]
  username varchar [not null] [note: '用户名称']
  password varchar [not null] [note: '用户密码']
  gender boolean [not null] [note: '性别：0 未知， 1男， 1 女']
  birthday date [note: '生日']
  last_login_time datetime [note: '最近一次登录时间']
  last_login_ip varchar [not null] [note: '最近一次登录IP地址']
  user_level boolean [note: '0 普通用户，1 VIP用户，2 高级VIP用户']
  nickname varchar [not null] [note: '用户昵称或网络名称']
  mobile varchar [not null] [note: '用户手机号码']
  avatar varchar [not null] [note: '用户头像图片']
  weixin_openid varchar [not null] [note: '微信登录openid']
  session_key varchar [not null] [note: '微信登录会话KEY']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
  Note: '用户名称'
}
