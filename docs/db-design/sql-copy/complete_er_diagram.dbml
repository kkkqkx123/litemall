}

Table litemall_goods {
  id integer [pk, not null]
  goods_sn varchar [ref, not null] [note: '商品编号']
  name varchar [not null] [note: '商品名称']
  category_id integer [ref] [note: '商品所属类目ID']
  brand_id integer [ref]
  gallery varchar [note: '商品宣传图片列表，采用JSON数组格式']
  keywords varchar [note: '商品关键字，采用逗号间隔']
  brief varchar [note: '商品简介']
  is_on_sale boolean [note: '是否上架']
  sort_order smallint
  pic_url varchar [note: '商品页面商品图片']
  share_url varchar [note: '商品分享海报']
  is_new boolean [note: '是否新品首发，如果设置则可以在新品首发页面展示']
  is_hot boolean [note: '是否人气推荐，如果设置则可以在人气推荐页面展示']
  unit varchar [note: '商品单位，例如件、盒']
  detail text [note: '商品详细介绍，是富文本格式']
  add_time datetime [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
