  expire_time datetime [note: '团购过期时间']
  status smallint [note: '团购规则状态，正常上线则0，到期自动下线则1，管理手动下线则2']
  add_time datetime [not null] [note: '创建时间']
  update_time datetime [note: '更新时间']
  deleted boolean [note: '逻辑删除']
